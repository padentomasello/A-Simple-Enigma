package enigma;

/** Class that represents a reflector in the enigma.
 *  @author cs61b-bz Daniel Paden Tomasello
 */
class Reflector extends Rotor {
    /** Refelector contructor which takes in Rotor int ID.  */
    public Reflector(int id) {
        super(id);
    }
    /** Returns a useless value; should never be called. */
    @Override
    int convertBackward(int unused) {
        throw new UnsupportedOperationException();
    }
    /** Reflectors do not advance. */
    @Override
    void advance() {
    }

}
